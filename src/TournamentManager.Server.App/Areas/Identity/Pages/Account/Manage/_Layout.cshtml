<!DOCTYPE html>
<html lang="cs">
<head>
    <title>@ViewData["Title"]</title>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no">
    <link rel="stylesheet" href="/assets/bootstrap/css/bootstrap.min.css">
        <link rel="stylesheet" href="/assets/fonts/nunito/css/Nunito.css">
        <link rel="stylesheet" href="/assets/css/site.css"/>
        <link rel="stylesheet" href="/assets/fonts/font-awesome/css/fontawesome-all.min.css">
        <link rel="icon" type="image/png" href="/favicon.ico"/>
</head>
<body id="page-top">
<div id="wrapper">
    <partial name="_ManageNav"/>
    <main class="d-flex flex-column" id="content-wrapper">
        <div id="content">
            @* <partial name="_ManageNavBar"/> *@
            <div class="container-fluid">
                @RenderBody()
            </div>
        </div>
    </main>
</div>
<script src="/assets/js/jquery-3.6.1.min.js"></script>
@RenderSection("Scripts", false);
<script src="/assets/bootstrap/js/bootstrap.min.js"></script>
<script defer src="/assets/js/theme.js"></script>
</body>
</html>